{
  "inputs": [
    {
      "type": "promptString",
      "id": "SECRET_APIKEY",
      "description": "Seitrace API Key",
      "password": true
    }
  ],
  "servers": {
    "Seitrace": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@seitrace/mcp"],
      "env": {
        "SECRET_APIKEY": "${input:SECRET_APIKEY}"
      }
    }
  }
}
